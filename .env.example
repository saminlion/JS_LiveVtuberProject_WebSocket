# =======================================
# LiveVtuberProject .env.example
# =======================================

# [REQUIRED] Path to your Google Cloud TTS service account key (JSON)
# In your actual .env file, provide the valid path to the key file
GOOGLE_APPLICATION_CREDENTIALS=./google-tts-key.json

# [OPTIONAL] WebSocket port (default: 8080)
PORT_WS=8080

# [OPTIONAL] OSC port for FaceCap input (default: 9000)
PORT_OSC=9000

# [OPTIONAL] HTTP server port for serving audio/json files (default: 3000)
PORT_HTTP=3000

# [OPTIONAL] Directory for storing generated audio and lip sync JSON files
AUDIO_DIR=public/audio